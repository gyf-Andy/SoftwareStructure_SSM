<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gyf.mapper.GoodsMapper">
                                                            <!--useGeneratedKeys="true" keyProperty="g_id"的作用是添加完信息后，返回信息的id-->
    <insert id="addGoods" parameterType="com.gyf.pojo.Goods" useGeneratedKeys="true" keyProperty="g_id">
        insert into goods(g_name,g_img) values (#{g_name},#{g_img})
    </insert>

</mapper>

